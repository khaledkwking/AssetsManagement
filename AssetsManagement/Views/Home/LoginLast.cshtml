        <div class="body" style="background-color:#dedede">
            <header id="header" data-plugin-options="{'stickyEnabled': true, 'stickyEnableOnBoxed': true, 'stickyEnableOnMobile': true, 'stickyStartAt': 55, 'stickySetTop': '-25px', 'stickyChangeLogo': true}">
                <div class="header-body">
                    <div class="header-container container">
                        <div class="header-row">
                            <div class="header-column">
                                <div class="header-row">
                                    <div class="header-logo">
                                        <a href="default.aspx">

                                            <img alt="GPFA EXAMS" height="54" src="~/New/img/logo.png">
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div class="header-column justify-content-end">
                                <div class="header-row">
                                    <div class="header-nav">
                                        <div class="header-nav-main header-nav-main-effect-1 header-nav-main-sub-effect-1">
                                            <nav class="collapse">
                                                <ul class="nav nav-pills" id="mainNav">
                                                    <li>

                                                        <div class="form-group">

                                                            <a href="@Url.Action(null,new { Lang = "ar" })" CssClass="kt-nav__link loginMargin">
                                                                @*<asp:LinkButton ID="ArabicLang" runat="server"  OnClick="ArabicLang_Click">*@
                                                                <span class="kt-nav__link-icon">
                                                                    <img src="~/assets/media/flags/021-Kuwait.svg" alt="" style="height: 20px;" />
                                                                </span>
                                                                <span class="kt-nav__link-text">عربي</span>
                                                                @*</asp:LinkButton>*@
                                                            </a>
                                                            <a href="@Url.Action(null,new { Lang = "en" })" CssClass="kt-nav__link loginMargin">
                                                                @*<asp:LinkButton ID="EnglishLang" runat="server" CssClass="kt-nav__link loginMargin" OnClick="EnglishLang_Click">*@
                                                                <span class="kt-nav__link-icon loginMargin">
                                                                    <img src="~/assets/media/flags/020-flag.svg" alt="" style="height: 20px; " />
                                                                </span>
                                                                <span class="kt-nav__link-text">English</span>
                                                                @*</asp:LinkButton>*@
                                                            </a>




                                                        </div>

                                                </ul>
                                            </nav>
                                        </div>

                                        <a class="btn header-btn-collapse-nav" data-toggle="collapse" data-target=".header-nav-main nav">
                                            <i class="fas fa-bars"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <div role="main" class="main" style="background-color:#dedede">

                <div class="slider-with-overlay">
                    <div class="slider-container rev_slider_wrapper" style="height: 660px;background-color:#dedede">
                        <div id="revolutionSlider" class="slider rev_slider" data-version="5.4.7" data-plugin-revolution-slider data-plugin-options="{'delay': 9000, 'gridwidth': 1170, 'gridheight': 660, 'responsiveLevels': [4096,1200,992,500]}">
                            <ul>
                                <li data-transition="fade">
                                    <img src="~/New/img/slides/slide-bg-full2.jpg"
                                         alt=""
                                         data-bgposition="center center"
                                         data-bgfit="contain"
                                         data-bgrepeat="no-repeat"
                                         class="rev-slidebg" />


                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="slider-contact-form">
                        <div class="container">
                            <div class="row justify-content-end">
                                <div class="col-lg-5">
                                    <div class="featured-boxes mt-0 mb-0">
                                        <div class="featured-box featured-box-primary mt-5">
                                            <div class="box-content">
                                              
                                                    @Html.Raw(ViewBag.SignIn)

                                               
                                                <div id="contactForm" runat="server">
                                                    <div class="form-row">
                                                        <div class="form-group col">
                                                           
                                                            @Html.Raw(ViewBag.UserTitle +"  *")
                                                            
                                                            @Html.TextBoxFor(m => m.UserName, "أسم المستخدم", new { @class = "form-control", @placeholder = ViewBag.UserTitle })
                                                        </div>
                                                    </div>
                                                    <div class="form-row">
                                                        <div class="form-group col">
                                                           
                                                            @Html.Raw(ViewBag.PassTitle + "  *")
                                                            @Html.PasswordFor(m => m.Password, new { @class = "form-control", @placeholder = ViewBag.PassTitle })

                                                        </div>
                                                    </div>

                                                    <div class="form-row">
                                                        <div class="form-group col-lg-4">
                                                            <input type="submit" id="LoginButton" value=@ViewBag.SignIn class="btn btn-primary" />
                                                        </div>

                                                    </div>
                                                    <div class="form-row">
                                                        <div class="form-group col-lg-12">
                                                            @*<div id="ErrorDIV" runat="server"></div>*@
                                                            @Html.Raw(TempData["Message"])
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


            <footer id="footer">
                <div class="footer-copyright">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-1">
                                <a href="default.aspx" class="logo">
                                    <img alt="GPFA EXAMS" class="img-fluid" src="~/New/img/logo-footer.png">
                                </a>
                            </div>
                            <div class="col-lg-7">
                                <p>© جهاز متابعة الاداء الحكومي 2021. جميع الحقوق محفوظة.</p>
                            </div>

                        </div>
                    </div>
                </div>
            </footer>
        </div>


